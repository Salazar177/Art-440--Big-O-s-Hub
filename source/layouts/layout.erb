<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "The Middleman" %></title>
    
    <%= stylesheet_link_tag "normalize", "all", "bootstrap.min" %>
    <%= javascript_include_tag  "all" %>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  
  <body class="<%= page_classes %>">
    <!-- ADD CONTENT HERE -->
    
    <!-- Grid -->
    <div class="container">
		<div class="row">
			<h1 class="col-md-6">Hello, world!</h1>
			<h2 class="col-md-6">Test test, 123456</h2>
		</div>
	  	<div class="row">
		    <div class="col-xs-4 col1">Column 1</div>
		    <div class="col-xs-4 col2">Column 2</div>
		    <div class="col-xs-4 col3">Column 3</div>
	  	</div>
  	</div>
    
    <%= yield %>
    
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
	    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
	    <!-- Include all compiled plugins (below), or include individual files as needed -->
	    <script src="javascripts/bootstrap.min.js"></script>
  </body>
</html>